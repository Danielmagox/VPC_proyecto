<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <BorderPane prefHeight="700.0" prefWidth="700.0">
         <top>
            <FlowPane hgap="5.0" prefHeight="200.0" prefWidth="200.0" vgap="5.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#abrirVentana" text="Abrir Imagen" />
                  <Button mnemonicParsing="false" onAction="#guardarImagen" prefHeight="31.0" prefWidth="155.0" text="Guardar Imagen" />
                  <Button mnemonicParsing="false" onAction="#mostrarHistograma" text="Histograma" />
                  <Button mnemonicParsing="false" onAction="#mostrarHistogramaAcumulativo" text="Histograma Acumulativo" />
                  <Button mnemonicParsing="false" onAction="#verInfoBasica" text="Información de la imagen" />
                  <Button mnemonicParsing="false" onAction="#escalaDeGrises" text="Pasar a escala de grises" />
                  <Button mnemonicParsing="false" onAction="#transformacionLinealTramos" text="Transformación lineal por tramos" />
                  <Button mnemonicParsing="false" onAction="#cambiarBrilloContraste" text="Cambiar brillo y contraste" />
                  <Button mnemonicParsing="false" onAction="#quitarRoi" text="Quitar ROI" />
               </children>
               <padding>
                  <Insets left="5.0" right="5.0" top="5.0" />
               </padding>
            </FlowPane>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
